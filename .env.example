# Example environment variables file
# Copy this to .env and fill in your actual API keys

# Together.ai API (Recommended - Cost-effective Llama models)
TOGETHER_API_KEY=your_together_ai_api_key_here

# OpenAI API (Optional)
OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face (for model downloads)
HUGGINGFACE_TOKEN=your_huggingface_token_here

# Data paths (if using external sources)
COVID_NEWS_API_KEY=your_news_api_key_here
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# Default settings
DEFAULT_LLM_PROVIDER=together
DEFAULT_MODEL=meta-llama/Meta-Llama-3-8B-Instruct

# Model storage (if using cloud storage)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_S3_BUCKET=your_s3_bucket_name

# Database (if storing results in database)
DATABASE_URL=your_database_url_here
